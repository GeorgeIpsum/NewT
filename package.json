{
  "name": "newt",
  "version": "0.1.0",
  "description": "a new tab page",
  "main": "newpage.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "bundle": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgeIpsum/newt.git"
  },
  "keywords": [
    "new-tab-page",
    "browser-extension"
  ],
  "author": "Ibrahim Saberi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/GeorgeIpsum/newt/issues"
  },
  "homepage": "https://github.com/GeorgeIpsum/newt#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "babel-loader": "^8.0.4",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
